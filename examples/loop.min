; program that prints "Hi" 10 times

.text
; start of the loop
start
; check if we've done all 10 loops
lda loops ; load the loops
brz done ; branch if 0

; print Hi
opc 'H'
opc 'i'
opc '\n' ; print new line

dec loops ; decrement the loop counter
jmp start ; next loop

; finished the loops
done
hlt

.data
loops dat 10